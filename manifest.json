{
  "manifest_version": 3,
  "name": "AI Integration",
  "version": "0.1",
  "author": "Devansh Kesan",
  "description": "A description about your Chrome Extension...",
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://maang.in/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources" : [
    {
      "resources": [
        "assets/bookmark.png",
        "assets/delete.png",
        "assets/play.png",
        "assets/ai3.png"
      ],
      "matches" : ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ai3.png",
      "24": "assets/ai3.png",
      "512": "assets/ai3.png"
    },
    "default_popup": "index.html",
    "default_title": "Falcon Hack AI"
  }
}
